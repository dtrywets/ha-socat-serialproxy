
  "name": "ha-socat-serialproxy",
  "version": "0.0.1",
  "slug": "ha-socat_serialproxy",
  "description": "Bridged serial device via TCP (e.g. ser2net)",
  "arch": ["aarch64", "armv7", "amd64"],
  "startup": "services",
  "boot": "auto",
  "hassio_api": false,
  "host_network": true,
  "options": {
    "remote_host": "192.168.20.20",
    "remote_port": 2004,
    "device_name": "/dev/ttyWMA0"
  },
  "schema": {
    "remote_host": "str",
    "remote_port": "int",
    "device_name": "str"
  },
  "privileged": ["SYS_ADMIN", "DAC_READ_SEARCH"],
  "tmpfs": true,
  "full_access": false
}
