
  "name": "socat-serialproxy",
  "version": "1.1.0",
  "slug": "socat_serialproxy",
  "description": "Expose a remote TCP serial port as a local device.",
  "arch": ["amd64", "aarch64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "REMOTE_HOST": "192.168.20.20",
    "REMOTE_PORT": 2004,
    "DEVICE_NAME": "/dev/ttyUSB20"
  },
  "schema": {
    "REMOTE_HOST": "str",
    "REMOTE_PORT": "int",
    "DEVICE_NAME": "str"
  },
  "init": false,
  "host_network": true,
  "privileged": ["SYS_ADMIN"],
  "devices": ["/dev"],
  "full_access": true
}
